{
  "id": "com.test.controller",
  "name": "ice美化助手",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "entryPoints": [
    {
      "type": "panel",
      "id": "pythonBridgePanel",
      "label": {
        "default": "ice美化助手"
      },
      "minimumSize": { "width": 320, "height": 260 },
      "preferredDockedSize": { "width": 360, "height": 320 },
      "preferredFloatingSize": { "width": 420, "height": 360 }
    }
  ],
  "requiredPermissions": {
    "localNetwork": "all",
    "network": {
      "domains": "all"
    },
    "clipboard": "readAndWrite",
    "localFileSystem": "fullAccess" 
  }
}       